{
  "name": "@kakegurui/shuffle",
  "version": "1.0.0",
  "description": "Shuffle utilities for Kakegurui Games",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "dist/**/*.js*",
    "dist/**/*.mjs*",
    "dist/**/*.d*"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "vitest",
    "lint": "eslint --fix --ext ts src",
    "format": "prettier --write --log-level=warn \"{src,tests}/**/*.{js,ts,json}\"",
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kakegurui-club/shuffle.git"
  },
  "keywords": [
    "shuffle",
    "kakegurui",
    "gamble",
    "gambling",
    "riffle",
    "gilbreath",
    "random",
    "deck",
    "cards",
    "pile",
    "deal",
    "casino"
  ],
  "author": "Ravener",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kakegurui-club/shuffle/issues"
  },
  "homepage": "https://github.com/kakegurui-club/shuffle#readme",
  "devDependencies": {
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "typescript-eslint": "^7.3.1",
    "vitest": "^1.4.0"
  }
}
